{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig-dev.json"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "arrow-body-style": "off",
    "@typescript-eslint/lines-between-class-members": "off"
  }
}
